{
  "name": "taskbask-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "npx tsc -b && vite build && npm run build-server",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean-server": "node scripts/clean-server.js",
    "start": "node server-dist/server.js",
    "build-server": "npm run clean-server && npx tsc --project tsconfig.server.json",
    "build-image": "npm install && npm run build && docker build -t oconnecp/taskbask.space-frontend:latest ./",
    "push-image": "docker push oconnecp/taskbask.space-frontend:latest",
    "build-and-push": "npm run build-image && npm run push-image"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.6.3",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}